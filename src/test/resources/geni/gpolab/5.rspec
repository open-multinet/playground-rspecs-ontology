<?xml version="1.0" encoding="UTF-8"?>
<rspec xmlns="http://www.protogeni.net/resources/rspec/2"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:delay="http://www.protogeni.net/resources/rspec/ext/delay/1"
       xsi:schemaLocation="http://www.protogeni.net/resources/rspec/2
                           http://www.protogeni.net/resources/rspec/2/request.xsd
                           http://www.protogeni.net/resources/rspec/ext/delay/1
                           http://www.protogeni.net/resources/rspec/ext/delay/1/request-delay.xsd
                           "
       type="request">
<node client_id="del-consumer-ds" exclusive="true">
    <sliver_type name="delay">
        <delay:sliver_type_shaping xmlns="http://www.protogeni.net/resources/rspec/ext/delay/1">
            <pipe dest="del-consumer-ds:ds" latency="20" capacity="12000" source="del-consumer-ds:consumer" />
            <pipe dest="del-consumer-ds:consumer" latency="20" capacity="12000" source="del-consumer-ds:ds" />
        </delay:sliver_type_shaping>
    </sliver_type>
    <interface client_id="del-consumer-ds:consumer" />
    <interface client_id="del-consumer-ds:ds" />
</node>
<node client_id="consumer" exclusive="true">
    <sliver_type name="raw-pc">
        <disk_image name="urn:publicid:IDN+emulab.net+image+emulab-ops:FEDORA15-STD" />
    </sliver_type>
    <services>
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-0.6.1-F15.tar.gz" install_path="/" />
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-setup.tar.gz" install_path="/tmp" />
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-atmos-F15.tar.gz" install_path="/" />
        <execute shell="sh" command="cd /tmp/ccnx-setup &amp;&amp; ./add-precip-routes ds" />
        <execute shell="sh" command="cd /tmp/ccnx-setup &amp;&amp; ./ccnx-setup ds 2" />
    </services>
    <interface client_id="consumer:ds">
        <ip netmask="255.255.255.0" type="ipv4" address="192.168.1.1" />
    </interface>
</node>
<node client_id="ds" exclusive="true">
    <sliver_type name="raw-pc">
        <disk_image name="urn:publicid:IDN+emulab.net+image+emulab-ops:FEDORA15-STD" />
    </sliver_type>
    <services>
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-0.6.1-F15.tar.gz" install_path="/" />
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-setup.tar.gz" install_path="/tmp" />
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/ccnx-atmos-F15.tar.gz" install_path="/" />
        <install url="http://www.cs.purdue.edu/homes/fahmy/geni/tarballs/atmos-data1.tar" install_path="/tmp" />
        <execute shell="sh" command="cd /tmp/ccnx-setup &amp;&amp; ./ccnx-setup ds 2" />
        <execute shell="sh" command="cd /tmp/ccnx-setup &amp;&amp; ./atmos-server pr_19020101_060000.nc" />
    </services>
    <interface client_id="ds:consumer">
        <ip netmask="255.255.255.0" type="ipv4" address="192.168.1.2" />
    </interface>
</node>
<link client_id="ds-consumer-consumer">
    <interface_ref client_id="consumer:ds" />
    <interface_ref client_id="del-consumer-ds:consumer" />
</link>
<link client_id="ds-consumer-ds">
    <interface_ref client_id="ds:consumer" />
    <interface_ref client_id="del-consumer-ds:ds" />
</link>
</rspec>
