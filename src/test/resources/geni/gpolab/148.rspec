<?xml version="1.0" ?>
 <!-- 
  This rspec will reserve the OpenFlow resources in the InstaGENI rack at Cornell
  used by the tupty3715 slice.
	URN: unspecified_AM_URN
	URL: https://foam.geni.it.cornell.edu:3626/foam/gapi/2
 -->
  
<rspec xmlns="http://www.geni.net/resources/rspec/3"
       xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:openflow="http://www.geni.net/resources/rspec/ext/openflow/3"
       xs:schemaLocation="http://www.geni.net/resources/rspec/3
           http://www.geni.net/resources/rspec/3/request.xsd
           http://www.geni.net/resources/rspec/ext/openflow/3
           http://www.geni.net/resources/rspec/ext/openflow/3/of-resv.xsd"
       type="request">

  <openflow:sliver description="OpenFlow Tutorial.">

    <openflow:controller url="tcp:CONTROLLER_IP:6633" type="primary" />

    <openflow:group name="cornell-instageni-1750">
      <openflow:datapath component_id="urn:publicid:IDN+openflow:foam:foam.geni.it.cornell.edu+datapath+06:d6:2c:59:e5:6b:4e:00" component_manager_id="urn:publicid:IDN+openflow:foam:foam.geni.it.cornell.edu+authority+am" />
    </openflow:group>

    <openflow:match>

      <openflow:use-group name="cornell-instageni-1750" />

      <openflow:packet>
        <openflow:dl_type value="0x800,0x806"/>
        <openflow:nw_dst value="NETWORK.0/24"/>
        <openflow:nw_src value="NETWORK.0/24"/>
      </openflow:packet>

    </openflow:match>

  </openflow:sliver>

</rspec>
